getgenv().Ngentot = {
    --AutoFarm [Third Sea]
	["Kaitun"] = false,
	["Select Weapons"] = nil or "Select Weapons",
    ["Auto New World"] = false,
    ["Auto Third World"] = false,
    ["Superhuman"] = false,
    ["Fruit"] = false,
}

function Load()
	if readfile and writefile and isfile and isfolder then
		if isfolder("ThunderZ") == false then
			makefolder("ThunderZ")
		end
		if isfile("/ThunderZ/Kaitun-" .. game.Players.LocalPlayer.DisplayName .. ".json") == false then
			writefile("/ThunderZ/Kaitun-" .. game.Players.LocalPlayer.DisplayName .. ".json", game:GetService("HttpService"):JSONEncode(getgenv().Ngentot))
		else
			local Decode = game:GetService("HttpService"):JSONDecode(readfile("/ThunderZ/Kaitun-" .. game.Players.LocalPlayer.DisplayName .. ".json"))
			for i,v in pairs(Decode) do
				getgenv().Ngentot[i] = v
			end
		end
	else
		warn("Failed Load")
		return false
	end
end
function Save()
	if readfile and writefile and isfile then
		if isfile("/ThunderZ/Kaitun-" .. game.Players.LocalPlayer.DisplayName .. ".json") == false then
			Load()
		else
			local Decode = game:GetService("HttpService"):JSONDecode(readfile("/ThunderZ/Kaitun-" .. game.Players.LocalPlayer.DisplayName .. ".json"))
			local Array = {}
			for i,v in pairs(getgenv().Ngentot) do
				Array[i] = v
			end
			writefile("/ThunderZ/Kaitun-" .. game.Players.LocalPlayer.DisplayName .. ".json", game:GetService("HttpService"):JSONEncode(Array))
		end
	else
		warn("Failed Save")
		return false
	end
end

Load()
Save()
