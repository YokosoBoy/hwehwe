spawn(function()
			while wait() do
				if getgenv().Ngentot["Auto Third World"] then
					local MyLevel = game.Players.localPlayer.Data.Level.Value
					if MyLevel >= 1500 and World2 and getgenv().Ngentot["Auto Third World"] then
						if getgenv().Ngentot["Kaitun"] == true then
							getgenv().Ngentot["Kaitun"] = false
						end
